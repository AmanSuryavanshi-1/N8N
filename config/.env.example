# n8n Configuration
# Copy this file to .env and fill in your actual values

# n8n Host Configuration
N8N_HOST=your-domain.com
N8N_PORT=5678
N8N_PROTOCOL=https

# Webhook URL (should match your public domain)
N8N_WEBHOOK_URL=https://your-domain.com/

# Encryption key for n8n (generate a secure random string)
N8N_ENCRYPTION_KEY=your-super-secret-encryption-key-here

# Timezone
GENERIC_TIMEZONE=America/New_York

# Authentication (recommended for production)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=your_username
N8N_BASIC_AUTH_PASSWORD=your_password

# External Service API Keys
TELEGRAM_BOT_TOKEN=123456789:ABCdefGhIjkLmNoPQRstuVwXyZ

# Additional n8n Settings
N8N_INSTALL_RECOMMENDED_PACKAGES=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=false
N8N_RUNNERS_ENABLED=true
NODE_ENV=production